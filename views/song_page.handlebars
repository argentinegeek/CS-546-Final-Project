<form action="/songs">
    <button type="submit" id="songs-page">Songs</button>
</form>

<form action="/artists">
    <button type="submit" id="artists-page">Artists</button>
</form>

<form action="/logout">
    <button type="submit" id="user-logout">Logout</button>
</form>
<a href="/private" id="website-home">Account</a>

<h1>{{songTitle}} by {{songArtist}}</h1>

<div>
    <p>Rating</p>
    <p>{{songRating}}</p>
</div>

<table>
    <tr>
        <th>Rating</th>
        <th>Username</th>
    </tr>
    <tr>
        <td>{{songRating}}</td>
        <td>{{username}}</td>
    </tr>
    <button type="submit">Like</button>
    <button type="submit">Dislike</button>
</table>

<table>
    <tr>
        <th>Genres</th>
        {{#each songGenres}}
        <td>{{this}}</td>
        {{/each}}
    </tr>
</table>

<h3>Recommended Songs</h3>
<table>
    <tr>
        <th>Song</th>
        <th>Artist</th>
    </tr>
    {{#each recommended}}
    <tr>
        <td>{{this.title}}</td>
        <td>{{this.artist}}</td>
    </tr>
     {{/each}}
</table>

